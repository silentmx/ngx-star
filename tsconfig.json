{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "module": "es2020",
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@silentmx/ngx-star": [
        "projects/silentmx/ngx-star"
      ],
      "@silentmx/ngx-star/*": [
        "projects/silentmx/ngx-star/*"
      ],
      "@angular/*": [
        "./node_modules/@angular/*"
      ]
    }
  }
}